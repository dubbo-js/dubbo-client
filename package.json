  "devDependencies": {
    "autod": "^2.7.1",
    "dubbo-registry": "^0.1.0",
    "egg-bin": "^4.2.0",
    "egg-ci": "^1.8.0",
    "eslint": "^4.6.0",
    "eslint-config-egg": "^5.1.0",
    "pedding": "^1.1.0",
